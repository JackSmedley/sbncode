#include "dissonant_higgs.fcl"
# service configuration
#
#include "services_icarus.fcl"


process_name: AnaTree

services:
{
  @table::icarus_basic_services
  TFileService: { fileName: "dissonant_higgs_icarus.root" }
  IFDH: {}
}

source: {
  module_type: EmptyEvent
}

physics:
{
  producers:{}
  filters:  {} 
  analyzers:{
    
    disshiggs: @local::dissonant_higgs
    
  } # analyzers
 
  ana:  [ disshiggs ]
 
  trigger_paths: [] 
  end_paths:     [ ana ] 
  
} # physics

physics.analyzers.disshiggs.Flux.M: 0.24
physics.analyzers.disshiggs.Flux.MixingAngle: 0.00001

services.NuRandomService.policy: "random"
