    # Resonance
    genie_MaCCRES_multisigma, genie_MvCCRES_multisigma, genie_MaNCRES_multisigma, genie_MvNCRES_multisigma,
    # Non-Resonance
    genie_NonRESBGvpCC1pi_multisigma, genie_NonRESBGvbarnCC1pi_multisigma, genie_NonRESBGvpNC1pi_multisigma, genie_NonRESBGvbarnNC1pi_multisigma, genie_NonRESBGvnCC1pi_multisigma, genie_NonRESBGvbarpCC1pi_multisigma, genie_NonRESBGvnNC1pi_multisigma, genie_NonRESBGvbarpNC1pi_multisigma, genie_NonRESBGvpCC2pi_multisigma, genie_NonRESBGvbarnCC2pi_multisigma, genie_NonRESBGvpNC2pi_multisigma, genie_NonRESBGvbarpNC2pi_multisigma, genie_NonRESBGvnCC2pi_multisigma, genie_NonRESBGvbarpCC2pi_multisigma, genie_NonRESBGvnNC2pi_multisigma, genie_NonRESBGvbarnNC2pi_multisigma,
